{
  "name": "ReadLater: Bookmarks in a queue",
  "version": "0.0.11",
  "manifest_version": 2,
  "description": "Simply click a button to queue a web-page for later reading. Use together with the ReadNow extension.",
  "icons": {
    "16": "images/icon-16.png",
    "96": "images/icon-96.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/chromereload.js",
      "scripts/background.js"
    ]
  },
  "permissions": [
    "bookmarks",
    "tabs"
  ],
  "page_action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    },
    "default_title": "ReadLater",
    "default_popup": "popup.html"
  }
}